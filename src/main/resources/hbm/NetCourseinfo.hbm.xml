<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.netcar.entity.NetCourseinfo" table="net_courseinfo">
        <id name="courseid">
            <column name="courseid" sql-type="int(11)"/>
            <generator class="native"/>
        </id>
        <property name="coursename">
            <column name="coursename" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="coursedate">
            <column name="coursedate" sql-type="date"/>
        </property>
        <property name="starttime">
            <column name="starttime" sql-type="timestamp"/>
        </property>
        <property name="stoptime">
            <column name="stoptime" sql-type="timestamp"/>
        </property>
        <property name="duration">
            <column name="duration" sql-type="int(11)"/>
        </property>
        <property name="createtime">
            <column name="createtime" sql-type="timestamp"/>
        </property>
        <property name="updatetime">
            <column name="updatetime" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>