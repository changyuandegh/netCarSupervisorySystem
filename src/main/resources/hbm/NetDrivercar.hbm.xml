<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.netcar.entity.NetDrivercar" table="net_drivercar">
        <id name="drivercarid">
            <column name="drivercarid" sql-type="int(11)"/>
            <generator class="native"/>
        </id>
        <property name="driverid">
            <column name="driverid" sql-type="int(11)"/>
        </property>
        <property name="carid">
            <column name="carid" sql-type="int(11)"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>