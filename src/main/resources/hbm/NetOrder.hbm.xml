        </property>
        <property name="depamount">
            <column name="depamount" sql-type="double(10,7)" precision="10" scale="7" not-null="true"/>
        </property>
        <property name="createtime">
            <column name="createtime" sql-type="timestamp"/>
        </property>
        <property name="updatetime">
            <column name="updatetime" sql-type="timestamp"/>
        </property>
        <property name="ordertrajectoryid">
            <column name="ordertrajectoryid" sql-type="bigint(20)"/>
        </property>
    </class>
</hibernate-mapping>