        <property name="vehicleno">
            <column name="vehicleno" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="destributetime">
            <column name="destributetime" sql-type="timestamp"/>
        </property>
        <property name="canceltime">
            <column name="canceltime" sql-type="timestamp"/>
        </property>
        <property name="canceltypecode">
            <column name="canceltypecode" sql-type="varchar(32)" length="32" not-null="true"/>
        </property>
        <property name="operator">
            <column name="operator" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="cartype">
            <column name="cartype" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="businesstype">
            <column name="businesstype" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="ordertype">
            <column name="ordertype" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="callphone">
            <column name="callphone" sql-type="int(11)"/>
        </property>
        <property name="passengerphone">
            <column name="passengerphone" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="passengercount">
            <column name="passengercount" sql-type="int(11)"/>
        </property>
        <property name="ordercalltype">
            <column name="ordercalltype" sql-type="int(11)"/>
        </property>
        <property name="acceptlongitude">
            <column name="acceptlongitude" sql-type="double(10,7)" precision="10" scale="7" not-null="true"/>
        </property>
        <property name="acceptlatitude">
            <column name="acceptlatitude" sql-type="double(10,7)" precision="10" scale="7" not-null="true"/>
