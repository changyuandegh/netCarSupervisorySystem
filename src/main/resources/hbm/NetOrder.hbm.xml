        </id>
        <property name="netorderid">
            <column name="netorderid" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="companyid">
            <column name="companyid" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="address">
            <column name="address" sql-type="int(11)"/>
        </property>
        <property name="departtime">
            <column name="departtime" sql-type="timestamp"/>
        </property>
        <property name="ordertime">
            <column name="ordertime" sql-type="timestamp"/>
        </property>
        <property name="passengernote">
            <column name="passengernote" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="departure">
            <column name="departure" sql-type="varchar(128)" length="128"/>
        </property>
        <property name="deplongitude">
            <column name="deplongitude" sql-type="double(10,7)" precision="10" scale="7"/>
        </property>
        <property name="deplatitude">
            <column name="deplatitude" sql-type="double(10,7)" precision="10" scale="7"/>
        </property>
        <property name="destination">
            <column name="destination" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="destlongitute">
            <column name="destlongitute" sql-type="double(10,7)" precision="10" scale="7" not-null="true"/>
        </property>
        <property name="destlatitude">
            <column name="destlatitude" sql-type="double(10,7)" precision="10" scale="7" not-null="true"/>
        </property>
        <property name="encrypt">
            <column name="encrypt" sql-type="int(11)"/>
        </property>
        <property name="state">
            <column name="state" sql-type="int(11)"/>
        </property>
        <property name="fareid">
            <column name="fareid" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="longitude">
            <column name="longitude" sql-type="double(10,7)" precision="10" scale="7" not-null="true"/>
        </property>
        <property name="latitude">
            <column name="latitude" sql-type="double(10,7)" precision="10" scale="7" not-null="true"/>
        </property>
        <property name="licenseid">
            <column name="licenseid" sql-type="varchar(32)" length="32" not-null="true"/>
